{
  "name": "frontend-template-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:dev": "env-cmd -f .env.development next build && next export",
    "build:prod": "env-cmd -f .env.production next build && next export",
    "build": "next build",
    "start": "next start",
    "lintjs": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lintjs:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "fix": "yarn format && yarn lint",
    "lintcss": "stylelint '**/*.{css,scss,sass}'",
    "lintcss:fix": "stylelint --fix '**/*.{css,scss,sass}'",
    "test": "jest",
    "generate": "plop"
  },
  "dependencies": {
    "@types/qs": "^6.9.7",
    "@types/react-transition-group": "^4.4.6",
    "amazon-cognito-identity-js": "^6.3.1",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "env-cmd": "^10.1.0",
    "next": "13.4.7",
    "nookies": "^2.5.2",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-slideshow-image": "^4.2.1",
    "react-transition-group": "^4.4.5",
    "reset.css": "^2.0.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "plop": "^3.1.2",
    "postcss": "8",
    "prettier": "^2.8.4",
    "prettier-stylelint-formatter": "^0.5.1-beta.2",
    "sass": "^1.58.0",
    "stylelint": "^15.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "5.1.6"
  },
  "volta": {
    "node": "18.2.0",
    "yarn": "1.22.19"
  }
}
